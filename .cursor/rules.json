{
  "auto_approve": [
    "bash scripts/run-full-tests.sh",
    "pnpm build",
    "pnpm lint",
    "pnpm test",
    "pnpm vitest",
    "pnpm playwright test",
    "pnpm --filter web vitest",
    "pnpm --filter web playwright test",
    "pnpm --filter web test",
    "pnpm --dir apps/web playwright test",
    "vercel logs",
    "vercel ls",
    "vercel inspect",
    "vercel --prod --confirm",
    "git status",
    "git log",
    "git show",
    "git diff",
    "git branch",
    "git stash",
    "git add",
    "git commit -m",
    "git push origin",
    "git pull origin",
    "git fetch",
    "git checkout -b",
    "git checkout",
    "git merge",
    "git rebase",
    "git grep",
    "git ls-files"
  ],
  "allow_scripts": [
    "scripts/run-full-tests.sh"
  ],
  "default_agents": [
    "/agents/dev-assistant.md",
    "/agents/mission.foundation.md"
  ],
  "session_timeout": 480
}
